<Window x:Class="NavigationService.Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NavigationService.Example"
        mc:Ignorable="d"
        Title="Huvudsida" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Detta är huvudsidan. Här skulle ni kunna lägga en huvudmeny" />
            <TextBlock Text="Har tre sidor som ni kan navigera till via knappar. Eller via respektive page" />
            <WrapPanel>
                <Button x:Name="GotoPage1" Content="Visa sida 1" MinWidth="34" Click="GotoPage1_Click"/>
                <Button x:Name="GotoPage2" Content="Visa Sida 2 (tom konstruktor)"  MinWidth="34" Click="GotoPage2_Click"/>
                <Button x:Name="GotoPage3" Content="Visa Sida 2 (som Cpu-spelare)"  MinWidth="34" Click="GotoPage3_Click"/>
            </WrapPanel>
            <Frame x:Name="mainFrame" />
        </StackPanel>
    </Grid>
</Window>
