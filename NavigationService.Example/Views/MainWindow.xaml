<Window x:Class="NavigationService.Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NavigationService.Example"
        mc:Ignorable="d"
        Title="Huvudsida" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Detta är huvudsidan. Här skulle ni kunna lägga en huvudmeny" />
            <TextBlock Text="Har tre sidor som ni kan navigera till via knappar. Eller via respektive page" />
            <WrapPanel>
                <Button Content="Visa sida 1" MinWidth="34" />
                <Button Content="Byter sida med en vymodell" Command="{Binding NavigationCommand}" MinWidth="34" />
                <Button Content="Visa Sida 2 (som Cpu-spelare)"  MinWidth="34" />
            </WrapPanel>
            <Frame x:Name="mainFrame" />
        </StackPanel>
    </Grid>
</Window>
