<local:BasePage x:TypeArguments="local:Page1ViewModel" x:Class="Navigering.Example.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Navigering.Example"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">

    <Grid>
        <StackPanel>
        <TextBlock Text="Nu är sida 1 laddad. Den har inga indataparametrar" />
        <Button Content="Gå till sida 2 med specifik modell" Command="{Binding NavigationCommand}"/>
        </StackPanel>
    </Grid>
</local:BasePage>
